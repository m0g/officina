{
  "name": "officina",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tailwind": "npx tailwindcss -i ./src/_assets/css/styles.css -o ./_site/css/styles.css --watch",
    "start": "concurrently \"yarn tailwind\" \"yarn dev\"",
    "dev": "eleventy --serve --watch",
    "prebuild": "npx tailwindcss -i ./src/_assets/css/styles.css -o ./_site/css/styles.css --minify",
    "build": "ELEVENTY_ENV=production eleventy --output=./_site",
    "format": "prettier --write \"src/**/*.+(js|json|css|md|rb)\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "yarn@1.22.19",
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-img": "3.1.8",
    "@ibm/plex": "^5.1.3",
    "autoprefixer": "^10.2.5",
    "eleventy-plugin-local-respimg": "^0.4.0",
    "husky": "^5.1.3",
    "imagemin-mozjpeg": "^9.0.0",
    "install": "^0.13.0",
    "leaflet": "^1.7.1",
    "lint-staged": "^10.5.4",
    "luxon": "^1.26.0",
    "postcss": "^8.4.34",
    "postcss-cli": "11.0.0",
    "prettier": "^2.2.1",
    "sharp": "^0.27.2",
    "tailwindcss": "3.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "htmx.org": "^1.9.10",
    "posthog-js": "^1.187.2"
  }
}
