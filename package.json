{
  "name": "officina",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf _site",
    "start": "npm-run-all clean --parallel dev:*",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --watch --quiet",
    "debug:eleventy": "DEBUG=Eleventy:* ELEVENTY_ENV=development eleventy --watch",
    "dev:modewebpack": "webpack --mode development --watch --config webpack.dev.js",
    "build": "npm-run-all clean --parallel prod:*",
    "prod:eleventy": "ELEVENTY_ENV=production eleventy --output=./_site",
    "prod:webpack": "webpack -p --config webpack.prod.js",
    "bsprod": "browser-sync start --server ./_site -w --no-open --no-notify --no-ghost-mode",
    "testbuild": "npm-run-all clean --parallel prod:webpack prod:eleventy bsprod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.12.3",
    "@ibm/plex": "^5.1.3",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^5.0.0",
    "eleventy-plugin-lazyimages": "^2.0.1",
    "eleventy-plugin-local-respimg": "^0.3.1",
    "file-loader": "^6.1.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.3",
    "imagemin-webpack-plugin": "^2.4.2",
    "install": "^0.13.0",
    "leaflet": "^1.7.1",
    "luxon": "^1.25.0",
    "mini-css-extract-plugin": "^1.0.0",
    "npm": "^6.14.8",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-cli": "^8.1.0",
    "postcss-loader": "^4.0.4",
    "responsive-loader": "^2.2.0",
    "rimraf": "^3.0.2",
    "sharp": "^0.26.2",
    "style-loader": "^2.0.0",
    "tailwindcss": "^1.9.4",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "glightbox": "3.0.5",
    "npm-check-updates": "^9.1.0"
  }
}
